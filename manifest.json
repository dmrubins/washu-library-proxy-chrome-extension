{
    "manifest_version": 2,
    "name": "Auto Library Proxy Access for HMS",
    "version": "2017.9.18",
    "author": "Nick Semenkovich and David Rubins",
    "description": "Automatically load journal websites using HMS Proxy.",
    "background": {
        "scripts": ["javascript/lso.js", "javascript/journals.js", "javascript/background.js"]
    },
    "icons": {
        "16":"images/schollidesign16.png",
        "48":"images/schollidesign48.png",
        "128":"images/schollidesign128.png"
    },
    "options_page": "views/options.html",
    "permissions": [
        "tabs", "webNavigation", "blocking"
    ],
    "browser_action": {
        "default_icon": "images/schollidesign19.png",
        "default_title": "Reload using HMS Proxy"
    },
    "web_accessible_resources": [ "images/schollidesign48.png" ]
}
