	"name": "WashU Library Proxy Access",
	"version": "0.5",
	"description": "Automatically load journal websites using Becker and Libproxy (BETA)",
	"background_page": "background.html",
	"icons": {
		"16":"wucrest16.png",
		"48":"wucrest48.png",
		"128":"wucrest128.png"
	},
	"options_page": "options.html",
	"permissions": [
		"tabs", "https://becker.wustl.edu/*", "https://update.epoxate.com/becker-extension/*"
	],
	"browser_action": {
		"default_icon": "wucrest19.png",
		"default_title": "Reload using WashU Proxy"
	}
}
